<idea-plugin>
    <id>org.example.FormatEntity</id>
    <name>FormatEntity</name>
    <vendor email="ying.fu@corp.to8to.com">yin</vendor>
    <depends>com.intellij.modules.lang</depends>
    <!--    <description><![CDATA[-->
    <!--    Enter short description for your plugin here.<br>-->
    <!--    <em>most HTML tags may be used</em>-->
    <!--    ]]></description>-->

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html

         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>
    <idea-version since-build="160.0"/>
    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="MainAction" class="com.yin.formatentity.MainAction" text="Format String as Entity"
                description="Format Swagger yapi string  as Entity">
            <add-to-group group-id="GenerateGroup" anchor="last"/>
        </action>

        <action id="PointAction" class="com.yin.pointutil.PointAction" text="Point Util">
            <add-to-group group-id="GenerateGroup" anchor="last"/>
        </action>
    </actions>
</idea-plugin>